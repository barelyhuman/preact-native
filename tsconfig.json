{
  "compilerOptions": {
    "baseUrl": ".",
    "checkJs": true,
    "jsx": "preserve",
    "lib": ["dom", "es5"],
    "skipLibCheck": true,
    "noEmit": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "moduleResolution": "node",
    "target": "ES2015",
    "outDir": "dist",
    "paths": {
      "@barelyhuman/preact-native": ["."],
      "@barelyhuman/preact-native/*": ["./*"]
    }
  },
  "exclude": [
    "node_modules",
    "dist",
    "node_modules",
    "./node_modules",
    "./node_modules/*"
  ]
}
